    {
      "cell_type": "code",
      "source": [
        "mx = np.mean(x) #mean of x\n",
        "my = np.mean(y) #mean of y\n",
        "mxy = np.mean(x*y)\n",
        "mx2 = np.mean(x*x)\n",
        "m = (mxy - my*mx)/(mx2-mx*mx)\n",
        "b = my - m*m_x\n",
        "print(m, b)\n",
        "#ordinary least squares\n"
